  "tokenColors": [
    {
      "scope": ["keyword"],
      "settings": { "foreground": "#FF6464" }
    },
    {
      "scope": ["storage.type"],
      "settings": { "foreground": "#D10000" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#B062FF" }
    },
    {
      "scope": ["entity.name.class"],
      "settings": { "foreground": "#DB3C3C" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": { "foreground": "#A8A8A8" }
    },
    {
      "scope": ["keyword.operator", "storage.modifier"],
      "settings": { "foreground": "#FF6464" }
    },
    {
      "scope": ["comment"],
      "settings": { "fontStyle": "italic", "foreground": "#8A8A8A" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#68E868" }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["punctuation"],
      "settings": { "foreground": "#FF8C00" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF6B68" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#FF6464" }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": { "foreground": "#A8A8A8" }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["heading.markdown"],
      "settings": { "foreground": "#FF6464" }
    },
    {
      "scope": ["markup.bold.markdown", "markup.bold"],
      "settings": { "fontStyle": "bold", "foreground": "#68E868" }
    },
    {
      "scope": ["markup.italic.markdown", "markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#68E868" }
    },
    {
      "scope": ["variable.language"],
      "settings": { "foreground": "#E16464" }
    },
    {
      "scope": ["meta.property-value"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": { "foreground": "#FF69B4" }
    },
    {
      "scope": ["meta.function-call.generic"],
      "settings": { "fontStyle": "bold", "foreground": "#FFD700" }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": { "foreground": "#AEFFAE" }
    },

    {
      "scope": ["constant.language"],
      "settings": { "foreground": "#BE5046" }
    },
    {
      "scope": ["support.function"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["support.type"],
      "settings": { "foreground": "#D84720" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#5F5F5F" }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#68E868" }
    }
  ],
  "semanticTokenColors": {
    "keyword": "#FF6464",
    "class": { "foreground": "#DB3C3C" },
    "method": { "foreground": "#FF6464" },
    "variable": "#A8A8A8",
    "number": "#B062FF",
    "operator": "#FF6464",
    "namespace": { "foreground": "#FFC74A" },
    "type": { "fontStyle": "italic", "foreground": "#D10000" }
  }
}
