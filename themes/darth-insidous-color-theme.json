  "tokenColors": [
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#BA3BE7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#9F54F6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E7C374",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#54B4F6",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FF4242"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#C4B6E3"
      }
    },
    {
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#D8C6FF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#CF1F1F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#FF7EE3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6E5C82"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#7EDEFF"
      }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#E7C374"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#D63030"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF2D2D"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#54B4F6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": {
        "foreground": "#C4B6E3"
      }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#E7C374"
      }
    },
    {
      "scope": ["heading.markdown"],
      "settings": {
        "foreground": "#FF4242",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold.markdown", "markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7EDEFF"
      }
    },
    {
      "scope": ["markup.italic.markdown", "markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7EDEFF"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.property-value"],
      "settings": {
        "foreground": "#E7C374"
      }
    },
    {
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": {
        "foreground": "#FF92DF"
      }
    },
    {
      "scope": ["meta.function-call.generic"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B71C1C"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#9F87FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FF7EE3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#54B4F6"
      }
    },
    {
      "scope": ["support.type"],
      "settings": {
        "foreground": "#FF4D4D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#9F87FF"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7EDEFF"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#54B4F6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#CF1F1F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#E7C374",
        "fontStyle": "bold"
      }
    }
