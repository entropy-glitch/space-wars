  "semanticTokenColors": {
    "keyword": "#CF1F1F",
    "class": {
      "foreground": "#54B4F6",
      "fontStyle": "italic bold"
    },
    "method": {
      "foreground": "#FF4242",
      "fontStyle": "bold"
    },
    "variable": "#C4B6E3",
    "number": "#E7C374",
    "operator": "#BA3BE7",
    "namespace": {
      "foreground": "#54B4F6",
      "fontStyle": "italic"
    },
    "type": {
      "foreground": "#9F54F6",
      "fontStyle": "italic"
    },
    "parameter": {
      "foreground": "#D8C6FF",
      "fontStyle": "italic"
    },
    "property": "#9F87FF",
    "enum": {
      "foreground": "#E7C374",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#54B4F6",
      "fontStyle": "italic"
    }
  }
}
