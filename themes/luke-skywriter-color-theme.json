  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": { "foreground": "#FF8C00", "fontStyle": "bold" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#5AF78E" }
    },
    {
      "scope": ["entity.name.class", "entity.other.inherited-class"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": ["keyword.operator", "storage.modifier"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["comment"],
      "settings": { "fontStyle": "italic", "foreground": "#858585" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["punctuation"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF5F56" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#FF8C00" }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["heading.markdown"],
      "settings": { "foreground": "#FF8C00" }
    },
    {
      "scope": ["markup.bold.markdown", "markup.bold"],
      "settings": { "fontStyle": "bold", "foreground": "#FFC74A" }
    },
    {
      "scope": ["markup.italic.markdown", "markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#5AF78E" }
    },
    {
      "scope": ["variable.language"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["meta.property-value"],
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": { "foreground": "#FF69B4" }
    },
    {
      "scope": ["meta.function-call.generic"],
      "settings": { "fontStyle": "bold", "foreground": "#FFD700" }
    },
    {
      "scope": ["constant.language"],
      "settings": { "foreground": "#5AF78E" }
    },
    {
      "scope": ["support.function"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["support.type"],
      "settings": { "foreground": "#00B0FF" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#D4D4D4" }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#5AF78E" }
    },
    {
      "scope": ["entity.name.decorator"],
      "settings": { "foreground": "#FF69B4", "fontStyle": "bold" }
    },
    {
      "scope": ["support.constant"],
      "settings": { "foreground": "#FFC74A" }
    },
    {
      "scope": ["meta.annotation"],
      "settings": { "foreground": "#FFD700", "fontStyle": "italic" }
    },
    {
      "scope": ["storage.type.function.arrow"],
      "settings": { "foreground": "#3399FF" }
    },
    {
      "scope": ["support.class"],
      "settings": { "foreground": "#FF8C00" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#FF8C00" }
    },
    {
      "scope": ["meta.import"],
      "settings": { "foreground": "#FF8C00", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword.control"],
      "settings": { "foreground": "#3399FF", "fontStyle": "bold" }
    },
    {
      "scope": ["storage.type.primitive"],
      "settings": { "foreground": "#3399FF" }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": { "foreground": "#FF8C00" }
    }
  ],
  "semanticTokenColors": {
    "keyword": "#00B0FF",
    "class": { "foreground": "#FFD700" },
    "method": { "foreground": "#00B0FF" },
    "variable": "#D4D4D4",
    "number": "#5AF78E",
    "operator": "#00B0FF",
    "namespace": { "foreground": "#FFD700" },
    "type": { "foreground": "#00B0FF" },
    "decorator": { "foreground": "#FF69B4", "fontStyle": "bold" },
    "constant": { "foreground": "#FFC74A" },
    "annotation": { "foreground": "#FFD700", "fontStyle": "italic" }
  }
}
