  },
  "tokenColors": [
    {
      "scope": ["keyword"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "scope": ["storage.type"],
      "settings": { "foreground": "#4B97D2" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#E0B0FF" }
    },
    {
      "scope": ["entity.name.class"],
      "settings": { "foreground": "#98E0E8" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#FFE0A0" }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": { "foreground": "#D8D8D8" }
    },
    {
      "scope": ["keyword.operator", "storage.modifier"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "scope": ["comment"],
      "settings": { "fontStyle": "italic", "foreground": "#6B8299" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#9BE0AE" }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": { "foreground": "#FFE0A0" }
    },
    {
      "scope": ["punctuation"],
      "settings": { "foreground": "#FFB280" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF9B9B" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": { "foreground": "#D8D8D8" }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": { "foreground": "#FFE0A0" }
    },
    {
      "scope": ["heading.markdown"],
      "settings": { "foreground": "#87CEEB" }
    },
    {
      "scope": ["markup.bold.markdown", "markup.bold"],
      "settings": { "fontStyle": "bold", "foreground": "#9BE0AE" }
    },
    {
      "scope": ["markup.italic.markdown", "markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#9BE0AE" }
    },
    {
      "scope": ["variable.language"],
      "settings": { "foreground": "#98E0E8" }
    },
    {
      "scope": ["meta.property-value"],
      "settings": { "foreground": "#FFE0A0" }
    },
    {
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": { "foreground": "#FFB0E8" }
    },
    {
      "scope": ["meta.function-call.generic"],
      "settings": { "fontStyle": "bold", "foreground": "#FFE0A0" }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": { "foreground": "#B0FFB0" }
    },
    {
      "scope": ["constant.language"],
      "settings": { "foreground": "#FF9B9B" }
    },
    {
      "scope": ["support.function"],
      "settings": { "foreground": "#FFE0A0" }
    },
    {
      "scope": ["support.type"],
      "settings": { "foreground": "#FFB280" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#6B8299" }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#9BE0AE" }
    }
  ],
  "semanticTokenColors": {
    "keyword": "#87CEEB",
    "class": { "foreground": "#98E0E8" },
    "method": { "foreground": "#87CEEB" },
    "variable": "#D8D8D8",
    "number": "#E0B0FF",
    "operator": "#87CEEB",
    "namespace": { "foreground": "#FFE0A0" },
    "type": { "fontStyle": "italic", "foreground": "#4B97D2" }
  }
}
