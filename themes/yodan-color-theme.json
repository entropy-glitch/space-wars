  "tokenColors": [
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#4B9783",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFD787",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#7AFFB2",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#BFD7E3"
      }
    },
    {
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#D4E6F2"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF9B5E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#87FFED",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6B8299"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#A2FFD0"
      }
    },
    {
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#FFD787"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#E7A3FF"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF7F7F"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": {
        "foreground": "#BFD7E3"
      }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#FFD787"
      }
    },
    {
      "scope": ["heading.markdown"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold.markdown", "markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#A2FFD0"
      }
    },
    {
      "scope": ["markup.italic.markdown", "markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A2FFD0"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FF9B5E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.property-value"],
      "settings": {
        "foreground": "#FFD787"
      }
    },
    {
      "scope": ["entity.other.attribute-name.pseudo-class"],
      "settings": {
        "foreground": "#F2B8FF"
      }
    },
    {
      "scope": ["meta.function-call.generic"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#66D9EF"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#A3FFD6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FF9B5E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#7AFFB2"
      }
    },
    {
      "scope": ["support.type"],
      "settings": {
        "foreground": "#87FFED",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#A2FFD0"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#7AFFB2"
      }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#E7A3FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#FFD787",
        "fontStyle": "bold"
      }
    }
