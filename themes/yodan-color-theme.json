    "method": {
      "foreground": "#66D9EF",
      "fontStyle": "bold"
    },
    "variable": "#BFD7E3",
    "number": "#FFD787",
    "operator": "#FF9B5E",
    "namespace": {
      "foreground": "#66D9EF",
      "fontStyle": "italic"
    },
    "type": {
      "foreground": "#87FFED",
      "fontStyle": "italic"
    },
    "parameter": {
      "foreground": "#D4E6F2",
      "fontStyle": "italic"
    },
    "property": "#66D9EF",
    "enum": {
      "foreground": "#FFD787",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#7AFFB2",
      "fontStyle": "italic"
